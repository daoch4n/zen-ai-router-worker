name = "gemini-openai-adapter"
main = "src/worker.mjs"
compatibility_date = "2025-01-01"
compatibility_flags = [ "nodejs_compat" ]

[placement]
mode = "smart"

[env.production]
preview_urls = false
workers_dev = false
routes = [
  { pattern = "gemini-openai-adapter.XXX.xyz", custom_domain = true }
]

vars.PASS = "XXX"

vars.KEY1 = "XXX"
vars.KEY2 = "XXX"
vars.KEY3 = "XXX"
vars.KEY4 = "XXX"

vars.DEFAULT_MODEL = "gemini-2.5-flash-preview-04-17"

[[env.production.d1_databases]]
binding = "MOCK_DB"
database_name = "mock-db-us"
database_id = "GUID"

[observability.logs]
enabled = true
